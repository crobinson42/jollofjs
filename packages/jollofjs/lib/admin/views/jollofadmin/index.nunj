<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Jollof Admin</title>
    <style>
        .loader {
            border: 16px solid #f3f3f3; /* Light grey */
            border-top: 16px solid rgb(0, 188, 212); /* Blue */
            border-radius: 50%;
            width: 60px;
            height: 60px;
            margin: 40px;
            animation: spin 1s linear infinite;
        }

        .loadingText {
            float: right;
        }

        @keyframes spin {
            0% {
                transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
            }
        }
    </style>
    <link href="/jollofstatic/flexboxgrid.min.css" rel="stylesheet" type="text/css">
    <link href="/jollofstatic/admin.css" rel="stylesheet" type="text/css">
    <link href="/jollofstatic/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="/static/jollof/admin.css" rel="stylesheet" type="text/css">
    <script
            src="https://code.jquery.com/jquery-3.1.1.min.js"
            integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
            crossorigin="anonymous">
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key={{ config.thirdParty.google.maps.apiKey }}&libraries=places">
    </script>
</head>
<body>
<div id="root">
    <span class="loadingText">Loading Jollof Admin...</span>
    <div class="loader"></div>
</div>
<script>
    {% if config.domains.admin %}
    var apiRoot = "{{ config.domains.admin }}";
    {% else %}
    var apiRoot = "{{ config.domains.default }}";
    {% endif %}

</script>
<script src="/jollofstatic/admin.bundle.js"></script>
</body>
</html>